{
  "name": "SpotOn Speeder",
  "description": "Allows speed control on Spotify",
  "version": "2.4.2",
  "manifest_version": 3,
  "author": "Kami",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://open.spotify.com/*"],
      "css": ["style.css"],
      "js": ["content-script.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["script.js"],
      "matches": ["https://open.spotify.com/*"]
    }
  ]
}
