{
  "name": "SpotOn",
  "version": "0.9.9.3",
  "description": "Overhaul Spotifys UI with SpotOn!",
  "icons": {
      "16": "icons/skinico16.png",
      "32": "icons/skinico32.png",
      "48": "icons/skinico48.png",
      "128": "icons/skinico128.png"
    },
  "manifest_version": 3,
  "content_scripts": [
   {
     "matches": ["https://open.spotify.com/*"],
     "js": ["content.js"]
   }
	],
  "action": {
    "default_popup":"popup.html",
    "default_icon": {     
      "16": "icons/skinico16.png",
      "32": "icons/skinico32.png",
      "48": "icons/skinico48.png",
      "128": "icons/skinico128.png"
    }
  }
}
